{
  "name": "g1plus",
  "version": "0.4.1",
  "manifest_version": 2,
  "description": "Download-Links und Wiedergabe von Ab-18-Inhalten nach Altersverifikation f√ºr GameOne.de auch innerhalb der Sperrzeiten",
  "icons": {
    "16":  "icons/icon_16.png",
    "128": "icons/icon_128.png"
  },
  "web_accessible_resources": [
    "icons/icon_64.png",
    "images/bg_agecheck.png",
    "images/bg_stripe.png",
    "images/warn_icon.png"
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.gameone.de/*",
    "http://videos.mtvnn.com/*",
    "http://de.esperanto.mtvi.com/*"
  ],
  "background": {
    "scripts": ["scripts/background_page.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.gameone.de/*"
      ],
      "css": [
        "style/style.css"
      ],
      "js": [
        "scripts/jquery.min.js",
        "scripts/konami.pack.js",
        "scripts/augment_page.js",
        "scripts/swfobject.js"
      ],
      "run_at": "document_end"
    }
  ]
}